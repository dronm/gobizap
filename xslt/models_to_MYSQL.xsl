<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:output method="text" indent="yes"			doctype-public="-//W3C//DTD XHTML 1.0 Strict//EN" 			doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/><xsl:variable name="DT_INT" select="'Int'"/><xsl:variable name="DT_STRING" select="'String'"/>			<xsl:variable name="DEF_SIZE_INT" select="11"/>			<xsl:template match="/">	<xsl:apply-templates select="metadata/model"/></xsl:template><xsl:template match="model[@sqlView='FALSE']">	CREATE TABLE IF NOT EXISTS `<xsl:value-of select="@dataBase"/>`.`<xsl:value-of select="@dataTable"/>`	(<xsl:apply-templates select="field"/>		PRIMARY KEY (<xsl:for-each select="field[@primaryKey='TRUE']">		<xsl:value-of select="@name"/>		<xsl:if test="not(position() = last())">			<xsl:value-of select="','"/>		</xsl:if>	</xsl:for-each>)	<!-- foreign keys-->	<xsl:for-each select="field[@fieldType='FT_LOOK_UP']">					,INDEX (<xsl:value-of select="lookUpKey/@id"/>),		FOREIGN KEY (<xsl:value-of select="lookUpKey/@id"/>)		REFERENCES `<xsl:value-of select="@dataBase"/>`.`<xsl:value-of select="@dataTable"/>` (<xsl:value-of select="lookUpKey/@lookUpId"/>)		ON DELETE <xsl:value-of select="@lookUpOnDelete"/>		ON UPDATE <xsl:value-of select="@lookUpOnUpdate"/>	</xsl:for-each>		)ENGINE = <xsl:apply-templates select="@mySQLEngine"/>CHARACTER SET utf8COLLATE utf8_general_ci;	</xsl:template><xsl:template match="model/field[not(@fieldType='FT_LOOK_UP')]">	<!-- field type && length -->	<xsl:variable name="data_type">		<xsl:choose>			<xsl:when test="@dataType=$DT_INT">				<xsl:value-of select="concat(' INT(',@length,')')"/>			</xsl:when>			<xsl:when test="@dataType=$DT_STRING">				<xsl:value-of select="concat(' VARCHAR(',@length,')')"/>			</xsl:when>			<xsl:otherwise>				<xsl:value-of select="concat(' UNSUPPORTED_DATA_TYPE(',@length,')')"/>							</xsl:otherwise>		</xsl:choose>	</xsl:variable>		<xsl:value-of select="@name"/> <xsl:value-of select="$data_type"/>	<!-- other attributes-->	<xsl:choose>		<xsl:when test="@required='TRUE'">			<xsl:value-of select="' NOT NULL'"/>		</xsl:when>		<xsl:otherwise>			<xsl:value-of select="' NULL'"/>		</xsl:otherwise>			</xsl:choose>	<xsl:if test="@defaultValue">		DEFAULT '<xsl:value-of select="@defaultValue"/>'	</xsl:if>		<xsl:if test="@autoInc='TRUE'">		<xsl:value-of select="' AUTO_INCREMENT'"/>	</xsl:if>	<xsl:if test="@unique='TRUE'">		<xsl:value-of select="' UNIQUE'"/>	</xsl:if>		<xsl:value-of select="',	'"/></xsl:template><xsl:template match="model/field[@fieldType='FT_LOOK_UP']">	<xsl:variable name="look_up_model_id" select="@lookUpModel"/>	<xsl:variable name="look_up_field_id" select="lookUpKey/@lookUpId"/>	<xsl:variable name="look_up_field" select="/metadata/model[@id=$look_up_model_id]/field[@name=$look_up_field_id]"/>		<!-- field type && length -->	<xsl:variable name="data_type">		<xsl:choose>			<xsl:when test="$look_up_field/@dataType=$DT_INT">				<xsl:value-of select="concat(' INT(',$look_up_field/@length,')')"/>			</xsl:when>			<xsl:when test="$look_up_field/@dataType=$DT_STRING">				<xsl:value-of select="concat(' VARCHAR(',$look_up_field/@length,')')"/>			</xsl:when>			<xsl:otherwise>				<xsl:value-of select="concat(' UNSUPPORTED_DATA_TYPE(',$look_up_field/@length,')')"/>							</xsl:otherwise>		</xsl:choose>	</xsl:variable>		<xsl:value-of select="lookUpKey/@id"/> <xsl:value-of select="$data_type"/>	<!-- other attributes-->	<xsl:choose>		<xsl:when test="$look_up_field/@required='TRUE'">			<xsl:value-of select="' NOT NULL'"/>		</xsl:when>		<xsl:otherwise>			<xsl:value-of select="' NULL'"/>		</xsl:otherwise>			</xsl:choose>	<xsl:if test="$look_up_field/@defaultValue">		DEFAULT '<xsl:value-of select="$look_up_field/@defaultValue"/>'	</xsl:if><!--		REFERENCES `<xsl:value-of select="@dataBase"/>`.`<xsl:value-of select="@dataTable"/>`	(<xsl:for-each select="lookUpKey">		<xsl:value-of select="@lookUpId"/>		<xsl:if test="not(position() = last())">			<xsl:value-of select="','"/>		</xsl:if>			</xsl:for-each>) ON DELETE <xsl:value-of select="@lookUpOnDelete"/>	ON UPDATE <xsl:value-of select="@lookUpOnUpdate"/>-->		<xsl:value-of select="',	'"/>	</xsl:template></xsl:stylesheet>